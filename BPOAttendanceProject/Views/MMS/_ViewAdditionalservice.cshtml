
<link href="../assets/global/plugins/bootstrap-modal/css/bootstrap-modal-bs3patch.css" rel="stylesheet" type="text/css" />
<link href="../assets/global/plugins/bootstrap-modal/css/bootstrap-modal.css" rel="stylesheet" type="text/css" />

@model BPOAttendanceProject.Models.MMSAdditional
<style>

#customers {
  font-family: Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 50%;
}

#customers td, #customers th {
  border: 1px solid #ddd;
  padding: 8px;
}



#customers tr:nth-child(even){background-color: #f2f2f2;}

#customers tr:hover {background-color: #ddd;}

#customers th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #4CAF50;
  color: white;
}
</style>


<div>
   
        <div class="modal-content">
            <div class="modal-body">
                <div>
                    <div>
                        <!-- BEGIN SAMPLE FORM PORTLET-->
                        <div class="portlet light bordered">
                            <div class="m-heading-1 border-green m-bordered">
                                <div class="caption">
                                    <i class="fa fa-edit"></i>
                                  
                                    View Details

                                </div>
                            </div>
                            <div class="portlet-body form">
                                <table class="table table-striped table-bordered table-hover table-checkable order-column" id="tbluser">
                                    <thead>
                                        <tr>


                                            <th style="background-color:#ADD8E6">Month Year</th>
                                            <th style="background-color:#ADD8E6">Team Size</th>
                                            <th style="background-color:#ADD8E6">Resigned</th>
                                            <th style="background-color:#ADD8E6">Target</th>
                                            <th style="background-color:#ADD8E6">Actual</th>
                                            <th style="background-color:#ADD8E6">ETO</th>
                                        </tr>
                                    </thead>
                                    <tbody>



                                        <tr class="odd gradeX">

                                            <td>@Model.Month @Model.Year</td>
                                            <td>@Model.TeamSize</td>
                                            <td>@Model.Resigned</td>
                                            <td>@Model.Target%</td>
                                            <td>@Model.Actual%</td>
                                            <td>@Model.ETO</td>
                                        </tr>
                                    


                                    </tbody>
                                </table>
                               
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                <div>Attrition : @Model.Attrition</div>
                                <table id="customers">

                                    <tr>
                                        <th>Department</th><th>BPO Online</th><th>MMS</th><th>Call center</th>
                                    </tr>
                                    <tr>
                                        <td>Size</td> <td>@Model.Size</td><td>@Model.MMSSize</td><td>@Model.CallSize</td>
                                    </tr>
                                    <tr>
                                        <td>Billable</td><td>@Model.Billable</td>  <td>@Model.MMSBillable</td><td>@Model.CallBillable</td>
                                    </tr> 
                                    <tr>
                                        <td>Non-Billable</td> <td>@Model.NonBillable</td><td> @Model.MMSNonBillable</td><td> @Model.CallNonBillable</td>
                                    </tr>
                                    <tr>
                                        <td>Teamwise ETO</td> <td>@Model.BPOETOINR</td><td>@Model.MMSETOINR</td><td>@Model.CallETOINR</td>
                                     </tr>
                                    <tr>
                                        <td>Teamwise ETO USD</td> <td>@Model.BPOETOUSD</td><td>@Model.MMSETOUSD</td><td>@Model.CallETOUSD</td>

                                    </tr>

                                   
                                   
                                </table>
                            </div>
                                <div class="col-md-6">Comments
                                     <div><table><tr><td>BPO:</td><td>@Model.BPOComments</td></tr><tr><td>MMS:</td><td>@Model.MMSComments</td></tr><tr><td>Call centre :</td><td>@Model.CallComments</td></tr></table></div>
                                        </div>
                                     </div>
                    
                       
                            

                        </div>
                        </div>
                        <!-- END SAMPLE FORM PORTLET-->
                    </div>

                </div>
            </div>
        
       <div class="modal-footer">
       <button type="button" class="btn dark btn-outline " data-dismiss="modal" >Close</button>  
  </div>
   
    <!-- /.modal-content -->
</div>
<!-- /.modal-dialog -->





