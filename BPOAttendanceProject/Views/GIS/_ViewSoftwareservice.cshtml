
<link href="../assets/global/plugins/bootstrap-modal/css/bootstrap-modal-bs3patch.css" rel="stylesheet" type="text/css" />
<link href="../assets/global/plugins/bootstrap-modal/css/bootstrap-modal.css" rel="stylesheet" type="text/css" />

@model BPOAttendanceProject.Models.SoftwareServices
<style>

#customers {
  font-family: Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 50%;
}

#customers td, #customers th {
  border: 1px solid #ddd;
  padding: 8px;
}



#customers tr:nth-child(even){background-color: #f2f2f2;}

#customers tr:hover {background-color: #ddd;}

#customers th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #4CAF50;
  color: white;
}
</style>


<div>
   
        <div class="modal-content">
            <div class="modal-body">
                <div>
                    <div>
                        <!-- BEGIN SAMPLE FORM PORTLET-->
                        <div class="portlet light bordered">
                            <div class="m-heading-1 border-green m-bordered">
                                <div class="caption">
                                    <i class="fa fa-edit"></i>
                                  
                                    View Details

                                </div>
                            </div>
                            <div class="portlet-body form">
                                 <table class="table table-striped table-bordered table-hover table-checkable order-column" id="tbluser">
                        <thead>
                            <tr>
                               
 
                                <th style="background-color:#ADD8E6">Month Year</th>
                                <th style="background-color:#ADD8E6">Software Services</th>
                                <th style="background-color:#ADD8E6">Internal Billing </th>
                                <th style="background-color:#ADD8E6">External Billing </th>
                                <th style="background-color:#ADD8E6">Total </th>
                                <th style="background-color:#ADD8E6">Resources</th> 
                                <th style="background-color:#ADD8E6">ETO in INR</th>
                                <th style="background-color:#ADD8E6">ETO in USD</th>
                            </tr>
                        </thead>
                        <tbody>
                           
                          
                                
                            <tr class="odd gradeX">
                               
                                <td>@Model.Month @Model.Year</td>
                                <td>@Model.softwareservices</td>
                                <td>₹@Model.internalbilling.ToString("#,##0")</td>
                                <td>₹@Model.externalbilling.ToString("#,##0")</td>
                                <td>₹@((Model.internalbilling-Model.externalbilling).ToString("#,##0"))</td>
                                 <td>@Model.Resources </td>
                                 <td>₹@Model.etoinINR.ToString("#,##0")</td>
                                <td>$@Model.etoinUSD.ToString("#,##0")</td>
                            </tr>
                       
                          

                        </tbody>
                    </table>
                               
                            </div>
<div>
  <table id="customers">
  <tr>
    <td>Attrition</td>
    <td>@Model.Attrition</td>
  </tr>

  <tr>
     <td>Billed Including Internal</td>
    <td>@Model.billedincluded</td>
  </tr>
   <tr>
    <td>Not Billed</td>
    <td>@Model.notbilled</td>
  </tr>
  <tr>
    <td>Best Performer</td>
    <td>@Model.bestperformer</td>
  </tr>
</table>
</div>
                            
                        @*<div>Attrition : @Model.Attrition</div>
                        <div>Billed Including Internal : @Model.billedincluded</div>
                        <div>Not Billed : @Model.notbilled</div>
                        <div>Best Performer : @Model.bestperformer</div>*@
                       
                            

                        </div>
                        </div>
                        <!-- END SAMPLE FORM PORTLET-->
                    </div>

                </div>
            </div>
        
       <div class="modal-footer">
       <button type="button" class="btn dark btn-outline " data-dismiss="modal" >Close</button>  
  </div>
   
    <!-- /.modal-content -->
</div>
<!-- /.modal-dialog -->





