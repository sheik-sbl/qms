
@model  BPOAttendanceProject.Models.User    

@if (Model.UserList.Count > 0)
{
    <table class="table table-striped table-bordered table-hover table-checkable order-column" id="tblPreprocess">
        <thead>
            <tr>
                <th style="width:5%">PSN</th>
                <th style="width:20%">Name </th>
                <th style="width:30%">Location</th>
              
               
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.UserList)
            {
               
                    
                          <tr class="odd gradeX">
                            <td>@item.Id</td>
                            <td>@item.FirstName</td>
                            <td>@item.location</td>
                          </tr>
              }

                   
             

        </tbody>
    </table>
    
}
else
{
    <div class="row">
        <div class="col-xs-12">
            <div class="alert alert-info">
                <strong>No records Found on this date</strong><br />
            </div>
        </div>
    </div>
}




<script>


    jQuery(function ($) {
        $('#tblPreprocess').dataTable({

            "aoColumns": [
                              null, null, null, null, null
            ]

        });

    });

</script>

