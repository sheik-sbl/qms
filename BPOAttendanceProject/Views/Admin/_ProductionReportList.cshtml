@model  BPOAttendanceProject.Models.ProductionDetailModel                     

 
@if (Model.lstproductionDetail.Count > 0)
{
   
    
    
     <table class="table table-striped table-bordered table-hover table-checkable order-column" id="tblPreprocess">
        <thead>
            <tr>
               

                <th style="width:5%">Project Code </th>
                <th style="width:5%">Event Code</th>
                <th style="width:5%">Date</th>
                <th style="width:5%">Target Production Record</th>
                <th style="width:5%">Actual Production Records</th>
                
                
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.lstproductionDetail)
            {

               
                 <tr class="odd gradeX">
                    <td>@item.projectcode</td>
                    <td>@item.eventcode</td>
                    <td>@item.date</td>
                    <td style="text-align:right" >@item.plannedprodrecord.ToString( "0.00" )</td>
                    <td style="text-align:right">@item.actualprodrecord</td>
                </tr>
                       
            }

          
        </tbody>
</table>
    
 
       
    
  
    
    
    
    
}
else
{
    <div class="row">
        <div class="col-xs-12">
            <div class="alert alert-info">
                <strong>No Daily Production details found on this date</strong><br />
            </div>
        </div>
    </div>
}

<script>


    jQuery(function ($) {
        $('#tblPreprocess').dataTable({

            "aoColumns": [
                              null, null, null, null, null

            ]

        });

    });

</script>

