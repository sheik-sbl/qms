
@model  BPOAttendanceProject.Models.NotexistModel    

@if (Model.NotexistList.Count > 0)
{
    <table class="table table-striped table-bordered table-hover table-checkable order-column" id="tblPreprocess">
        <thead>
            <tr>
                <th style="width:5%">Projectcode</th>
                <th style="width:20%">Eventcode </th>
                <th style="width:5%">Process</th>
                <th style="width:5%">location</th>
                <th style="width:5%">Type</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.NotexistList)
            {
               
                    
                          <tr class="odd gradeX">
                            <td>@item.Projectcode</td>
                            <td>@item.Eventcode</td>
                            <td>@item.Process</td>
                            <td>@item.location</td>
                            <td>@item.prtype</td>
                          </tr>
              }

                   
             

        </tbody>
    </table>
    
}
else
{
    <div class="row">
        <div class="col-xs-12">
            <div class="alert alert-info">
                <strong>No records Found on this date</strong><br />
            </div>
        </div>
    </div>
}

@if (Model.cnt == 0)
{
<div class="row">
        <div class="col-xs-12">
            <div class="alert alert-info">
                <strong>Please add monthly configuration</strong><br />
            </div>
        </div>
    </div>
}


<script>


    jQuery(function ($) {
        $('#tblPreprocess').dataTable({

            "aoColumns": [
                              null, null, null, null, null
            ]

        });

    });

</script>

