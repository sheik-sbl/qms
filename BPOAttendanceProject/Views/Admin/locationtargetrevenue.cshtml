@using System.Data
@model DataTable
@{
   
   
    int count = 0;
   double datacount = 0.0;
    int rowcount = 0;
   
}

<table class="table table-striped table-bordered table-hover table-checkable order-column" id="tblPreprocess" >
    <tr>
        @foreach (DataColumn col in Model.Columns)
        {
            count = count + 1; 
            if (count == 1 )
             {
            
         <th  style="width:8%;text-align:center">@col.ColumnName</th>
        }
        else
         {
             <th style="width:8%;text-align:center">@col.ColumnName</th>
            
            }       
        }

    </tr>
    @if (ViewBag.KAKKANAD != 0)
    {
         <tr><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
         <tr><td><span style="font-weight:bold">Total Till Date</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}", ViewBag.KAKKANAD)</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}", ViewBag.KNPY)</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}",ViewBag.MDS)</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}",ViewBag.MNS)</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}",ViewBag.MQC)</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}",ViewBag.TVM)</span></td><td>@string.Format("{0:#.00}",ViewBag.TOTAL)</td> </tr>
    }
        else
        {
         <tr><td></td><td></td><td></td><td></td><td></td><td></td></tr>
         <tr><td><span style="font-weight:bold">Total Till Date</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}", ViewBag.KNPY)</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}",ViewBag.MDS)</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}",ViewBag.MNS)</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}",ViewBag.MQC)</span></td><td><span style="font-weight:bold">@string.Format("{0:#.00}",ViewBag.TVM)</span></td><td>@string.Format("{0:#.00}",ViewBag.TOTAL)</td> </tr>
    
    }
    @foreach (DataRow row in Model.Rows)
    {
       
         <tr>
            @foreach (DataColumn col in Model.Columns)
            {
          
             <td>@row[col.ColumnName]</td>
             
             }
            
        </tr>
       
    }
</table>