                                    <td>reminder</td>
                                    <td>@Model.reminder</td>
                                    <td>Phone</td>
                                    <td>@Model.Phone</td>
                                </tr>
                                <tr>
                                    <td>REMARKS</td>
                                    <td>@Model.REMARKS</td>
                                    <td>Details</td>
                                    <td>@Model.Details</td>
                                    <td>REMARKS6</td>
                                    <td>@Model.REMARKS6</td>
                                    <td>REMARKS10</td>
                                    <td>@Model.REMARKS10</td>
                                </tr>
                                <tr>
                                    <td>Probing</td>
                                    <td>@Model.Probing</td>
                                    <td>REMARKS4</td>
                                    <td>@Model.REMARKS4</td>
                                    <td>Timeline</td>
                                    <td>@Model.Timeline</td>
                                    <td>Grammar</td>
                                    <td>@Model.Grammar</td>
                                </tr>
                                <tr>
                                    <td>REMARKS2</td>
                                    <td>@Model.REMARKS2</td>
                                    <td>Solution</td>
                                    <td>@Model.Solution</td>
                                    <td>listening</td>
                                    <td>@Model.listening</td>
                                    <td>REMARKS11</td>
                                    <td>@Model.REMARKS11</td>
                                </tr>
                                <tr>
                                    <td>Professionalism</td>
                                    <td>@Model.Professionalism</td>
                                    <td>REMARKS12</td>
                                    <td>@Model.REMARKS12</td>
                                    <td>tools</td>
                                    <td>@Model.tools</td>
                                    <td>rude</td>
                                    <td>@Model.rude</td>
                                </tr>
                                <tr>
                                    <td>Tagging2</td>
                                    <td>@Model.Tagging2</td>
                                    <td>mistakes</td>
                                    <td>@Model.mistakes</td>
                                    <td>Total</td>
                                    <td>@Model.Total</td>
                                    <td>Action Taken</td>
                                    <td>@Model.ActionTaken</td>
                                </tr>
                            </table>
                        </div>

                    </div>
                </div>
                <!-- END SAMPLE FORM PORTLET-->
            </div>

        </div>
    </div>

    <div class="modal-footer">
        <button type="button" class="btn dark btn-outline " data-dismiss="modal">Close</button>
    </div>

    <!-- /.modal-content -->
</div>
<!-- /.modal-dialog -->
